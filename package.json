{
  "name": "codai",
  "publisher": "fbehrens",
  "repository": {
    "type": "git",
    "url": "https://github.com/fbehrens/codai.git"
  },
  "displayName": "codai",
  "description": "use openai Chat Completion api wht the roles function,sytem,user nad assistant",
  "version": "0.7.0",
  "prettier": {
    "tabWidth": 2,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Codai",
      "properties": {
        "codai.model": {
          "type": "string",
          "default": "gpt-4o",
          "enum": [
            "gpt-3.5-turbo",
            "gpt-4",
            "gpt-4-32k",
            "gpt-4-0125-preview",
            "gpt-4o"
          ],
          "description": "openai model"
        },
        "codai.detail": {
          "type": "string",
          "default": "low",
          "enum": [
            "low",
            "high"
          ],
          "description": "openai image resolution"
        }
      }
    },
    "commands": [
      {
        "command": "codai.chat_completion",
        "title": "Codai: Chat Completion"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "rm -rf out && npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "mocha --watch --watch-files src/lib/*.ts,src/libtest/*.ts",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "testold": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "20.x",
    "@types/vscode": "^1.82.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vscode/test-electron": "^2.3.4",
    "esbuild": "^0.21.3",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "openai": "^4.6.0"
  }
}
